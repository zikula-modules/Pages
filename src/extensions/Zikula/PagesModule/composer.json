{
    "name": "zikula/pages-module",
    "version": "4.1.0",
    "description": "ZikulaPagesModule generated by ModuleStudio 1.4.0.",
    "type": "zikula-module",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Zikula Team",
            "email": "info@ziku.la",
            "homepage": "https://ziku.la",
            "role": "owner"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\PagesModule\\": "" }
    },
    "require": {
        "php": ">=7.2.5"
    },
    "require-dev": {
    },
    "extra": {
        "zikula": {
            "core-compatibility": ">=3.0.0 <4.0.0",
            "class": "Zikula\\PagesModule\\ZikulaPagesModule",
            "displayname": "Pages",
            "url": "pages",
            "icon": "fas fa-file-alt",
            "capabilities": {
                "admin": {
                    "route": "zikulapagesmodule_page_adminindex"
                },
                "user": {
                    "route": "zikulapagesmodule_page_index"
                },
                "categorizable": {
                    "entities": [
                        "Zikula\\PagesModule\\Entity\\PageEntity"
                    ]
                }
            },
            "securityschema": {
                "ZikulaPagesModule::": "::",
                "ZikulaPagesModule:ItemListBlock:": "Block title::",
                "ZikulaPagesModule:ItemBlock:": "Block title::",
                "ZikulaPagesModule:Page:": "Page ID::",
                "ZikulaPagesModule::Ajax": "::"
            }
        }
    },
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
